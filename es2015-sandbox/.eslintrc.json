{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "parserOptions" : {
      "sourceType" : "module"
    }
    ,
    "extends": "standard",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": "off" ,
        "space-before-function-paren" : "off",
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
